package ex01;

public class Conversion {

	public static void main(String[] args) {
		
		double d;
		
		int sum = 100;
		d = sum;
		System.out.println(d);
		System.out.println(sum);

		short s1 = 32000;
		int i1;
		i1 = s1;
		System.out.println(s1);
		System.out.println(i1);  // 자동 형 변환
		
		int i2 = 32000;
		short s2;
		s2 = i2s;
		System.out.println(i2);
		System.out.println(s2);
		
	}

}
