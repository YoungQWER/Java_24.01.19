
public class switch01 {

	public static void main(String[] args) {
		char grade = 'B';
		
		switch (grade) {
		case 'a':
			System.out.println("매우 우수");
			break;
		case 'b':
			System.out.println("우수");
			break;
		case 'c':
			System.out.println("좋음");
			break;
		case 'd':
			System.out.println("좀 더 열심히");
			break;
		case 'f':
			System.out.println("미흡");
			break;			
		default:
			System.out.println("잘못된 등급");
		}

	}

}
