package ex04;

public class 로또 {

	public static void main(String[] args) {
		
		int[] arrNum = new int[45];
		
		//1~45 숫자중에서 랜덤하게 6개 추출.......
		for(int i=0; i<arrNum.length; i++) {
			int num = (int)(Math.random()*100)/2;
			arrNum[i] = num;
		
			System.out.print("\s" + arrNum[i]);
		}
			

	}

}
